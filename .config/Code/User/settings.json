{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.tabSizing": "shrink",
    "files.trimTrailingWhitespace": true,
    "editor.renderWhitespace": "all",
    "editor.tokenColorCustomizations": {
        "comments": "#575757"
    },
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#313131",
        "editor.selectionHighlightBackground": "#313131",
        "editor.findMatchBackground": "#5f1111",
        "editor.findMatchHighlightBackground": "#1b5826",
        "tab.activeBackground": "#413f2c"
    },
    "breadcrumbs.enabled": false,
    // "python.pythonPath": "python2.7",
    // "editor.occurrencesHighlight": false,
}